SCHEMA >
    `processed_at` DateTime64(3) `json:$.processed_at`,
    `cluster_id` String `json:$.cluster_id`,
    `cluster_summary` String `json:$.cluster_summary`,
    `representative_texts` Array(String) `json:$.representative_texts[:]`,
    `report_count` Int32 `json:$.report_count`

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(processed_at)"
ENGINE_SORTING_KEY "processed_at, cluster_id"