DESCRIPTION >
    Count of reports per model_name, ordered by count descending.
    Counts all report events individually.

NODE model_counts
SQL >
    SELECT
        model_name,
        count() as report_count
    FROM llm_events
    WHERE event_type = 'report'
        AND model_name != ''
        AND model_name IS NOT NULL
    GROUP BY model_name
    ORDER BY report_count DESC

TYPE endpoint